# Puerto en el que corre el servidor
PORT=XXXX

# Credenciales de Base de Datos MongoDB
USERDB=your_db_user
PASSWORDDB=your_db_password

# Cadena de conexión a MongoDB Atlas
MOMGODB_CNN=mongodb+srv://your_db_user:your_db_password@cluster0.xxxxx.mongodb.net/CafeDB

# Clave secreta para JWT (generar una cadena aleatoria larga)
SECRETORPRIVATEKEY=your_secret_key_here_minimum_64_characters_recommended

# Credenciales de Google OAuth
GOOGLE_CLIENT_ID=your_google_client_id.apps.googleusercontent.com
GOOGLE_SECRET_ID=your_google_secret_id

# Configuración de Cloudinary (almacenamiento de imágenes)
CLOUDINARY_URL=cloudinary://your_api_key:your_api_secret@your_cloudinary_name
CLOUDINARY_APIKEY=your_cloudinary_api_key
CLOUDINARY_APISECRET=your_cloudinary_api_secret